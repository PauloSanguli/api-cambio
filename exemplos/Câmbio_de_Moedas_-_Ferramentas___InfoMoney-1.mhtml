@charset "utf-8";

.ticker-container { display: flex; background-color: rgb(0, 0, 0); }

.ticker-container .ticker-carroussel { flex-grow: 1; overflow-x: scroll; }

.ticker-container .ticker-carroussel #ticker-content { white-space: nowrap; animation: 40s linear 8s infinite normal none running marqueeTicker; display: inline-block; }

.ticker-container .ticker-carroussel #ticker-content a { padding: 0px 16px; display: inline-flex; align-items: center; height: 40px; border-right: 1px solid rgba(255, 255, 255, 0.2); flex-grow: 1; }

.ticker-container .ticker-carroussel #ticker-content a div.points, .ticker-container .ticker-carroussel #ticker-content a div.title { font-size: 11px; line-height: 16px; color: rgb(255, 255, 255); }

.ticker-container .ticker-carroussel #ticker-content a div.title { font-weight: 700; text-transform: uppercase; }

.ticker-container .ticker-carroussel #ticker-content a div.points { display: flex; }

.ticker-container .ticker-carroussel #ticker-content a div.points .value { margin: 0px 8px; font-weight: 700; }

.ticker-container .ticker-carroussel #ticker-content a div.points i.material-icons { font-size: 16px; margin-right: 4px; }

.ticker-container .ticker-carroussel #ticker-content a div.points .spread { font-weight: 600; }

.ticker-container .ticker-carroussel #ticker-content a.positivo div.points .spread, .ticker-container .ticker-carroussel #ticker-content a.positivo div.points i.material-icons { color: rgb(30, 185, 128); }

.ticker-container .ticker-carroussel #ticker-content a.negativo div.points .spread, .ticker-container .ticker-carroussel #ticker-content a.negativo div.points i.material-icons { color: rgb(255, 82, 82); }

.ticker-container .ticker-carroussel #ticker-content div.aditional { float: right; position: absolute; display: inline-block; }

.ticker-container .ticker-carroussel #ticker-content:hover { animation-play-state: paused; }

.ticker-container .ticker-carroussel::-webkit-scrollbar { width: 0px !important; height: 0px !important; }

.ticker-container .ticker-open-account { display: flex; align-items: center; }

.ticker-container .ticker-open-account a { align-items: center; background: linear-gradient(90deg, rgb(88, 114, 252) 0px, rgb(89, 2, 232) 100%); color: rgb(255, 255, 255); display: flex; height: 100%; padding: 0px 1rem; white-space: nowrap; }

.ticker-container .ticker-open-account img { margin: 0px 0px 0px 12px; }

.category .widget-ticker-highlow { padding: 0px 16px; }

div.widget-ticker-highlow { background-color: rgb(255, 255, 255); min-width: 100%; }

div.widget-ticker-highlow div.items { display: flex; justify-content: space-between; }

div.widget-ticker-highlow div.items div.table-quote h3 { font-weight: 600; font-size: 14px; line-height: 20px; color: rgb(33, 33, 33); letter-spacing: -0.5px; text-transform: uppercase; margin: 0px; }

div.widget-ticker-highlow div.items div.table-quote table { margin: 16px 0px 0px; width: 100%; }

div.widget-ticker-highlow div.items div.table-quote table thead tr th { font-weight: 400; font-size: 12px; line-height: 16px; color: rgb(108, 117, 125); }

div.widget-ticker-highlow div.items div.table-quote table tbody tr td:nth-child(2), div.widget-ticker-highlow div.items div.table-quote table thead tr th:nth-child(2) { text-align: center; font-weight: 400; }

div.widget-ticker-highlow div.items div.table-quote table tbody tr td:nth-child(3), div.widget-ticker-highlow div.items div.table-quote table thead tr th:nth-child(3) { text-align: right; }

div.widget-ticker-highlow div.items div.table-quote table td { font-weight: 600; font-size: 13px; line-height: 22px; letter-spacing: -0.3px; padding: 4px 0px; }

div.widget-ticker-highlow div.items div.table-quote table tr { border-bottom: 1px solid rgba(0, 0, 0, 0.12); }

div.widget-ticker-highlow div.items div.table-quote table#high tr td:nth-child(2) { color: rgb(30, 185, 128); }

div.widget-ticker-highlow div.items div.table-quote table#low tr td:nth-child(2) { color: rgb(255, 82, 82); }

div.widget-ticker-highlow div.items div.table-quote:nth-child(1) { margin: 0px 21.33px 0px 0px; flex-basis: calc(33.33% - 21.33px); }

div.widget-ticker-highlow div.items div.table-quote:nth-child(2) { margin: 0px 10.66px; flex-basis: calc(33.33% - 21.33px); }

div.widget-ticker-highlow div.items div.table-quote:nth-child(3) { margin: 0px 0px 0px 21.33px; flex-basis: calc(33.33% - 21.33px); }

@media screen and (max-width: 991px) {
  .ticker-container .ticker-open-account a { padding: 0px 0.5rem; }
  div.widget-ticker-highlow div.items { flex-direction: column; }
  div.widget-ticker-highlow div.items div.table-quote:nth-child(1) { margin: 0px; }
  div.widget-ticker-highlow div.items div.table-quote:nth-child(2), div.widget-ticker-highlow div.items div.table-quote:nth-child(3) { margin: 24px 0px 0px; }
}

@keyframes marqueeTicker { 
  0% { transform: translate(0px, 0px); }
  100% { transform: translate(-100%, 0px); }
}
